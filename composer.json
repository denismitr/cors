{
    "name": "denismitr/laracors",
    "description": "Cross Origin Resource Sharing Middleware for Laravel or Lumen",
    "license": "MIT",
    "keywords": ["laravel", "lumen", "cors", "middleware", "http"],
    "type": "library",
    "authors": [
        {
            "name": "Denis Mitrofanov",
            "email": "denis.mitr@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0 || ^7.1",
        "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x",
        "symfony/http-foundation": "~2.7|~3.0",
        "symfony/http-kernel": "~2.7|~3.0",
        "illuminate/http": "5.1.x|5.2.x|5.3.x|5.4.x"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^0.9.9",
        "squizlabs/php_codesniffer": "^3.0",
        "satooshi/php-coveralls": "^1.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Denismitr\\Laracors\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
